   vexactsize 0
   style editor_quad
   {
    PanelWidgetClass pnl_bg {
     ignorepointer 1
     position 0 0
     size 0.99 0.99
     halign center_ref
     valign center_ref
     hexactpos 0
     vexactpos 0
     hexactsize 0
     vexactsize 0
     style rover_sim_black
     {
      TextWidgetClass Title {
       ignorepointer 1
       position 0 0
       size 0.5 0.111
       halign center_ref
       hexactpos 0
       vexactpos 0
       hexactsize 0
       vexactsize 0
       style Normal
       text "My Custom Boring 1995 looking like Menu"
       font "gui/fonts/Metron-Bold28"
       "exact text" 1
       "size to text h" 1
       "size to text v" 1
       "text halign" center
       "text valign" center
      }
      ScrollWidgetClass Scroller {
       ignorepointer 0
